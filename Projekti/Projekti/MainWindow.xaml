<Window x:Class="Projekti.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekti"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="101*"/>
            <ColumnDefinition Width="99*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="Kello" HorizontalAlignment="Left" Margin="270,357,0,0" VerticalAlignment="Top" Width="104" Height="33" Grid.Column="1"/>
        <Label x:Name="Paiva" FontSize="14" HorizontalAlignment="Left" Margin="259,395,0,16" Width="115" Grid.Column="1"/>
        <Button Content="Asetukset" Margin="317,13,14,375" RenderTransformOrigin="2.683,-0.951" Grid.Column="1">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.066"/>
                    <TranslateTransform X="-0.077" Y="-0.162"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Label Content="Kirjoita tehtävä alla olevaan lootaan" HorizontalAlignment="Left" Height="34" Margin="113,354,0,0" VerticalAlignment="Top" Width="209" RenderTransformOrigin="0.495,1.187"/>
        <Label Content="TO-DO LIST" HorizontalAlignment="Left" Height="47" Margin="61,20,0,0" VerticalAlignment="Top" Width="218" FontSize="24"/>
        <Label Content="Kesken olevat tehtävät" HorizontalAlignment="Left" Height="24" Margin="18,72,0,0" VerticalAlignment="Top" Width="261"/>
        <Label Content="Valmiit tehtävät" HorizontalAlignment="Left" Height="29" Margin="20,209,0,0" VerticalAlignment="Top" Width="259"/>
        <Button x:Name="poista" Content="Poista" HorizontalAlignment="Left" Height="85" Margin="298,246,0,0" VerticalAlignment="Top" Width="84" Click="poista_Click"/>
        <Button x:Name="siirrä" Content="Siirrä" HorizontalAlignment="Left" Height="73" Margin="298,109,0,0" VerticalAlignment="Top" Width="84" Click="siirrä_Click"/>
        <Button x:Name="lisää" Content="Lisää" HorizontalAlignment="Left" Height="71" Margin="27,388,0,0" VerticalAlignment="Top" Width="81" Click="lisää_Click"/>
        <ListBox x:Name="kesken" HorizontalAlignment="Left" Height="108" Margin="20,101,0,0" VerticalAlignment="Top" Width="259" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <ListBox x:Name="valmiit" HorizontalAlignment="Left" Height="111" Margin="20,238,0,0" VerticalAlignment="Top" Width="259" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <TextBox x:Name="textbox" HorizontalAlignment="Left" Height="87" Margin="113,381,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="199"/>

    </Grid>
</Window>
